package com.tcs.assessment.week2;

public class AccountEx {
	public static void main(String[] args) {
		Account sa = new SavingsAccount(1, "ICICI", 0, "Savings");
		Account ca = new CurrentAccount(2, "SBI", 0, "Current");
		
		sa.deposit(1000);
		sa.withdraw(500);
		sa.getBal();
		
		ca.deposit(100000);
		ca.withdraw(45000);
		ca.getBal();
	}
}
