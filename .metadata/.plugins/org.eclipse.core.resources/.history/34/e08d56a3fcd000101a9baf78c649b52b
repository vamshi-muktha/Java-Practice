package com.tcs.handson.constructor;

public class Student extends Person {
	
	int stno;
	String stAdd;
	public Student(String fname, String lname, int stno, String stAdd) {
		super(fname, lname);
		this.stno = stno;
		this.stAdd = stAdd;
	}
	@Override
	public String toString() {
		return "Student [stno=" + stno + ", stAdd=" + stAdd + ", fname=" + fname + ", lname=" + lname + "]";
	}
	
	
}
