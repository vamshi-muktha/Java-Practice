package com.tcs.sample.abstracthandson;

public class ExAbstract {
	public static void main(String[] args) {
		Employee e1 = new Programmer(1, "Mehedi");
		e1.jobs();
		e1.timings();
		
		e1 = new Analyst(2, "Tuskin");
		e1.jobs();
		e1.timings();
		
		
		e1 = new Manager(4, "Rubel");
		e1.jobs();
		e1.timings();
	}
}
