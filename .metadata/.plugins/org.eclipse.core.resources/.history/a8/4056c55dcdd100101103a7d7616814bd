package com.tcs.sample.abstracthandson;

public class Programmer extends Employee {
	
	
	

	public Programmer(int eno, String ename) {
		super(eno, ename);
	}

	@Override
	void jobs() {
		// TODO Auto-generated method stub
		System.out.println("Programmer Job");
	}

	@Override
	void timings() {
		// TODO Auto-generated method stub
		System.out.println("Programmer Timings");
	}

}
