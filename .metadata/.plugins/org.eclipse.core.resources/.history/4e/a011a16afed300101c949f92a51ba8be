package com.tcs.handson.inheritence;

public class Student extends Person {
	
	int stno;
	String stAdd;
	public Student(String fname, String lname, int stno, String stAdd) {
		super(fname, lname);
		this.stno = stno;
		this.stAdd = stAdd;
	}
	void display() {
		super.display();
		System.out.println(stno + " " + stAdd);
	}
	@Override
	public String toString() {
		return "Student [stno=" + stno + ", stAdd=" + stAdd + ", fname=" + getFname() + ", lname=" + getLname() + "]";
	}
	
	
}
